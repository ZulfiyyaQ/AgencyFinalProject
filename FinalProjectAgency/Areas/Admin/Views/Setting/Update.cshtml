@model UpdateSettingVM
<div class="main-panel">
    <div class="content-wrapper">
        <div class="row">
            <div class="col-md-6 grid-margin stretch-card">
                <div class="card">
                    <div class="card-body">
                        <h4 class="card-title">Update Setting</h4>

                        <form class="forms-sample" method="post" enctype="multipart/form-data">
                            <div class="form-group">

                                <input asp-for="Key" type="text" class="form-control" id="exampleInputUsername1" placeholder="Key">
                                <span asp-validation-for="Key"></span>
                            </div>
                            <div class="form-group">

                                <input asp-for="Value" type="text" class="form-control" id="exampleInputUsername1" placeholder="Value">
                                <span asp-validation-for="Value"></span>
                            </div>

                            <button type="submit" class="btn btn-primary me-2">Submit</button>
                            <a asp-action="index" asp-controller="setting" class="btn btn-light">Cancel</a>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

